<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>thumbnailZoom@dadler.github.com</em:id>
    <em:name>Thumbnail Zoom Plus</em:name>
    <em:description>Shows full image when you hover over a thumbnail.  Works with many sites including Amazon, Baidu Images, Bing Images, Facebook, Flickr, Google+, Google Images, IMDb, LinkedIn, Netflix, Pinterest, Reddit, Taobao, Tumblr, Twitter, Yandex.ru, YouTube, Weibo, Wikipedia, WordPress, Yahoo Images, and many more.</em:description>
    
    <em:version>3.0beta2</em:version>
    
    <em:creator>David Adler and Andres Hernandez Monge</em:creator>
    <em:contributor>Alexey Kuznetsov</em:contributor>
    <em:contributor>Vlastimil Ovčáčík</em:contributor>
    <em:homepageURL>https://addons.mozilla.org/en-US/firefox/addon/thumbnail-zoom-plus/</em:homepageURL>
    <em:iconURL>chrome://thumbnailzoomplus/skin/images/icon64.png</em:iconURL>
    <em:optionsURL>chrome://thumbnailzoomplus/content/options.xul</em:optionsURL>
    <em:type>2</em:type>
	
    <em:localized>
      <Description>
        <em:locale>sv-SE</em:locale>
        <em:description>Visar den fullständiga bilden när du mushovrar över en bildminiatyr.  Fungerar med webbplatser som Amazon, Baidu Images, Bing Images, Facebook, Flickr, Google+, Google Bilder, IMDb, LinkedIn, Netflix, Pinterest, Reddit, Taobao, Tumblr, Twitter, Yandex.ru, YouTube, Weibo, Wikipedia, WordPress, och många fler.</em:description>
        <em:translator>Mikael Hiort af Ornäs</em:translator>
      </Description>
    </em:localized>

    <em:localized>
      <Description>
        <em:locale>es</em:locale> <!-- specifically, es-VE -->
        <em:description>Muestra imagen completa al colocar puntero sobre su miniatura. Funciona en numerosos sitios como Amazon, Baidu Images, Bing Images, Facebook, Flickr, Google+, Google Images, IMDb, LinkedIn, Netflix, Pinterest, Reddit, Taobao, Tumblr, Twitter, Yandex.ru, YouTube, Weibo, Wikipedia, WordPress, Yahoo Images y muchos otros.</em:description>
        <em:translator>Ricardo A. Rivas - rivica@gmail.com"</em:translator>
      </Description>
    </em:localized>
  
    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>29.0a1</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <em:targetApplication>
      <!-- SeaMonkey -->
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>2.2.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
